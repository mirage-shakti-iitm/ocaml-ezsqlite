	riscv64-unknown-linux-gnu-ar qc libezsqlite_stubs.a  lib/sqlite3_new.o lib/ezsqlite_stubs.o
	riscv64-unknown-linux-gnu-ranlib libezsqlite_stubs.a 

clean:
	rm lib/*.o
	rm *.a
